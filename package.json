{
  "name": "js-dev-environment",
  "version": "1.0.0",
  "description": "Javascript dev environment boilerplate",
  "author": "Jacopo Carassai",
  "jest": {
    "verbose": true
  },
  "scripts": {
    "start": "npx babel-node server/server.js",
    "open:src": "babel-node server/server.js",
    "lint": "eslint client",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "test": "jest",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "npx babel-node server/build.js",
    "postbuild": "babel-node server/distServer.js",
    "generate-mock-data": "babel-node server/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch client/src/api/db.json --port 3001"
  },
  "dependencies": {
    "core-js": "^3.12.1",
    "eslint-plugin-jest": "^23.17.1",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.3",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "cors": "^2.8.4",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.11.2"
  }
}
